{
  "dart.flutterSdkPath": ".fvm/flutter_sdk",
  "search.exclude": {
    "**/.fvm": true
  },
  "files.watcherExclude": {
    "**/.fvm": true
  },
  "[dart]": {
    "editor.defaultFormatter": "Dart-Code.dart-code",
    "editor.formatOnSave": true
  },
  "cSpell.words": [
    "autofocus",
    "autovalidate",
    "checkmark",
    "davigmacode",
    "endtemplate",
    "Irfan",
    "LTRB",
    "Memoizer",
    "Taufik",
    "Vigma"
  ],
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    ".fvm"
  ]
}